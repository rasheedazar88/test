  function starType(type){

    if (type == "type1") {
         var starRatingIcon = '<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Star_x5F_1" x="0px" y="0px" viewBox="0 0 100 100" style="enable-background:new 0 0 100 100;" xml:space="preserve" fill="#F0C14C"><polygon points="50,75.9 80.3,96.2 70.4,61.1 99,38.6 62.6,37.1 50,3 37.4,37.1 1,38.6 29.6,61.1 19.7,96.2 "/></svg>';
    } else if (type == "type2") {
         var starRatingIcon = '<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Star_x5F_2" x="0px" y="0px" viewBox="0 0 100 100" style="enable-background:new 0 0 100 100;" xml:space="preserve"><g><polygon fill="#F0C14C" points="68.6,60.4 80.2,96 49.9,74 19.7,96 31.3,60.4 1,38.5 38.4,38.5 49.9,2.9 61.5,38.5 98.9,38.5 68.6,60.4 "/></g></svg>';
    } else if (type == "type3") {
         var starRatingIcon = '<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Star_x5F_3" x="0px" y="0px" viewBox="0 0 100 100" style="enable-background:new 0 0 100 100;" xml:space="preserve" fill="#F0C14C"><polygon points="98.9,38.5 65.1,33.6 49.9,2.9 34.8,33.6 1,38.5 25.5,62.3 19.7,96 49.9,80.1 80.2,96 74.4,62.3 "/></svg>';
    } else if (type == "type4") {
