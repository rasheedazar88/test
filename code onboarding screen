  setting();
  appendData(); 
  owlSlider();

  function ScrollBottom(){
    $t = $('.item-content'); 
    $t.scroll(function(){      	
        if($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
          $(this).parent().addClass('scroll_reach_bottom');
        } else {
          $(this).parent().removeClass('scroll_reach_bottom');
        }
    });
  }

  $(document).ready(function(){
    ScrollBottom(); 
    $('.starRating-Wrapper').click(function(){
      document.getElementById('review_body_1010_type2').scrollIntoView({behavior: "smooth"})
    });
    
    var svgHeight = $(".reviewPage-topSvg").height();
    $(".reviewSlider-Wrapper").css("margin-top", svgHeight);
    
  });

  $(document).on('click', '.item-content-scroll-btn', function(event) {
    $target = $(this).parent().parent().find('.item-content');
  });

</script>  
