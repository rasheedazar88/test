<script>
  
  var avgStarCount = 0;
  var totReviews = 0; 
  var settings;
  var layoutType; 

  function appendData(){
      var data = $( "#customer_review" ).find("input").serialize();
      $.ajax({
        type: "POST",
        url: 'https://waltt-review-server.herokuapp.com/get_product_review',
        async : false,
        dataType: 'json',
        data: data,
        success:function(obj) {
            if(obj){
                var nietos = [];
                $.each(obj.data, function( key, item ) {
                  nietos.push(JSON.parse(item.product_review));
                });

                if(nietos.length !== 0){
                    settings = JSON.parse($('.review_settings').val());   
                    layoutType = settings.layoutType; 
                    var htmlContent = '<div id="review_body_1010_type2" class="reviewSlider '+nietos.length+'" starBgColor = "'+settings.starBgCol+'" actStarBgColor="'+settings.selectedStarBgCol+'" data-loop="'+settings.autoLoop+'" data-loop-duration="'+settings.autoLoopDuration+'" data-arrow="'+settings.arrowType+'" data-stars="'+settings.starType+'">'
                                        + '<div class="reviewPage-topSvg"><svg version="1.1" id="Arrow-Template" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 2000 85" style="enable-background:new 0 0 2000 85;" xml:space="preserve"><polygon points="0,0 0,85 2000,85 2000,0 1000.2,82 "></polygon></svg></div>'
                                        + '<div class="reviewSlider-Wrapper">'
                                        + '<div class="reviewSlider-header">'
                                            +'<button class="leaveReviewBtn modal-toggle">Leave a Review</button>'
                                            + '<'+settings.headerTag+' class="header-title">'+settings.headerText+'</'+settings.headerTag+'>'
                                            + '<div class="header-description">'
                                            + '<span class="starRatings"></span><span><a>Based on '+nietos.length+' reviews</a></span><span class="writeReview"><a>Write a review</a></span>'
                                            +'</div>'
                                            + '<span class="cv__line"></span>'
                                        + '</div>'
                                        + '<div class="reviewSlider-content">'                							
                                            + '<div class="content-reviews owl-carousel owl-theme">';

                    var totstarCount = 0; 
                  	totReviews = nietos.length; 
                    $.each(nietos, function( key, item ){

                      var num = parseInt(item.rating ? item.rating : 0);

                      totstarCount += num; 
                      var ext = item.image_url.substring(item.image_url.lastIndexOf(".")+1); 
                      if(ext == 'jpg' || ext == 'jpeg' || ext == 'gif' || ext == 'png'){
                          var mediaFile = '<img src='+item.image_url+' alt='+key+' />'
                      }else{
                          var mediaFile = '<video autoPlay loop muted><source className="uploded_video_src" src='+item.vedio_url+'/></video>'
                      }

                      htmlContent += '<div class="reviews-item">'
                                        +'<div class="item-image">'
                                        + mediaFile
                                        +'</div>'

                                        +'<div class="review-item-content">'
                                            +'<div class="item-header">'
                                              +'<h3 class="item-header-title">'+item.title+'</h3>'
                                              +'<span class="item-header-authorName">' + item.author + ''
                                              +'<span class="item-header-verified">'
                                              +'<i class="fa fa-check-circle"></i>verified'
                                              +'</span>'
                                              +'</span>'

                                              +'<div class="starRatings" data-colorCount="'+item.rating+'" data-color="'+settings.starBgCol+'" data-bgColor = "'+settings.selectedStarBgCol+'"></div>'
                                            +'</div>'
                                            +'<div class="item-content">'
                                            +'<div class="item-content-wrap">' + item.body + '</div>'
                                              + '<div class="item-content-scroll"><span class="item-content-scroll-btn"><svg class="Icon Icon--arrow-bottom" role="presentation" viewBox="0 0 21 11"> <polyline fill="none" stroke="currentColor" points="0.5 0.5 10.5 10.5 20.5 0.5" stroke-width="1.25"></polyline> </svg></span></div>'
                                              + '<div class="slide_below_bg"><div class="slide_below_wrap"><svg xmlns="http://www.w3.org/2000/svg" width="100%" viewBox="0 0 514 50"><defs><radialGradient id="dropshadow_2-ab" cx="50.146%" cy="0%" r="512.498%" fx="50.146%" fy="0%" gradientTransform="matrix(.09728 0 0 .09928 .453 0)"><stop offset="0%" stop-opacity=".8"/><stop offset="35.487%" stop-opacity=".5"/><stop offset="71.351%" stop-opacity=".25"/><stop offset="100%" stop-opacity="0"/></radialGradient></defs><rect width="514" height="50" fill="url(#dropshadow_2-ab)" fill-rule="evenodd" style=" fill: url(#dropshadow_2-ab); "/></svg></div></div>'
                                            +'</div>'
                                        +'</div>'
                                    +'</div>';

                    });

                    avgStarCount = (totstarCount / nietos.length).toFixed(0); 
					
                    htmlContent += '</div> </div></div> </div>'; 
                
                    $('#customer_product_review').append(htmlContent);
                    document.querySelector('.stars-inner').style.width = (avgStarCount * 25)+'px'; 
                    var startype = $('.reviewSlider').attr('data-stars');
                    starType(startype);
                }
            }  
        }  
        });

        var starRatings = $('.starRatings'); 
        setTimeout(function(){
            var likeCount, Color, inherit; 
            $(".starRatings").each(function(index, element, like_count){
                  likeCount = $(this).attr('data-colorCount'); 
                  Color = $(this).attr('data-bgColor');
                  inherit = $(this).attr('data-color');

                  $(this).find('svg *').css("fill", inherit);           

                  if(likeCount == 1){
                    $(this).find('svg:nth-child(1) *').css("fill", Color);
                  } else if(likeCount == 2){
                    $(this).find('svg:nth-child(1) *').css("fill", Color);           
                    $(this).find('svg:nth-child(2) *').css("fill", Color);           
                  }  else if(likeCount == 3){
                    $(this).find('svg:nth-child(1) *').css("fill", Color);           
                    $(this).find('svg:nth-child(2) *').css("fill", Color);           
                    $(this).find('svg:nth-child(3) *').css("fill", Color);           
                  }  else if(likeCount == 4){
                    $(this).find('svg:nth-child(1) *').css("fill", Color);           
                    $(this).find('svg:nth-child(2) *').css("fill", Color);           
                    $(this).find('svg:nth-child(3) *').css("fill", Color);           
                    $(this).find('svg:nth-child(4) *').css("fill", Color);           
                  }  else if(likeCount == 5){
                    $(this).find('svg:nth-child(1) *').css("fill", Color);           
                    $(this).find('svg:nth-child(2) *').css("fill", Color);           
                    $(this).find('svg:nth-child(3) *').css("fill", Color);           
                    $(this).find('svg:nth-child(4) *').css("fill", Color);           
                    $(this).find('svg:nth-child(5) *').css("fill", Color);           
                  }
            }); 

            $('.header-description').find('svg *').css("fill", inherit); 

                if(avgStarCount == 1){
                  $('.header-description').find('svg:nth-child(1) *').css("fill", Color); 
                } else if(avgStarCount == 2){
                  $('.header-description').find('svg:nth-child(1) *').css("fill", Color); 
                  $('.header-description').find('svg:nth-child(2) *').css("fill", Color);         
                } else if(avgStarCount == 3){
                  $('.header-description').find('svg:nth-child(1) *').css("fill", Color); 
                  $('.header-description').find('svg:nth-child(2) *').css("fill", Color);         
                  $('.header-description').find('svg:nth-child(3) *').css("fill", Color);         
                } else if(avgStarCount == 4){
                  $('.header-description').find('svg:nth-child(1) *').css("fill", Color); 
                  $('.header-description').find('svg:nth-child(2) *').css("fill", Color);         
                  $('.header-description').find('svg:nth-child(3) *').css("fill", Color);         
                  $('.header-description').find('svg:nth-child(4) *').css("fill", Color);         
                } else if(avgStarCount == 5){
                  $('.header-description').find('svg:nth-child(1) *').css("fill", Color); 
                  $('.header-description').find('svg:nth-child(2) *').css("fill", Color);         
                  $('.header-description').find('svg:nth-child(3) *').css("fill", Color);         
                  $('.header-description').find('svg:nth-child(4) *').css("fill", Color);         
                  $('.header-description').find('svg:nth-child(5) *').css("fill", Color);         
                }

      }, 1000); 
  }

  async function owlSlider(){
    var type = $('.reviewSlider').attr('data-arrow'); 
    var startype = $('.reviewSlider').attr('data-stars'); 
    var owl_custom_arrow = arrowType(type);
    var starRatingsIcon = starType(startype ? startype : 'type1');
	
	
    $(".content-reviews").owlCarousel({
      center: totReviews > 3 ? false : true, 
      loop: totReviews > 3 ? true : false, 
      items: 3,
      autoPlay: true,
      dotsEach: true,
      lazyLoad: true, 
      margin: 0, 
      nav: true, 
      responsive: {
        0: {items: 1}, 
        768: {items: layoutType == 'layouttype1' ? 2 : 3}, 
        1024: {items: layoutType == 'layouttype1' ? 3 : 5} 
      }  
    });
    $(".owl-nav span").remove(); 
    $(".owl-nav button").empty(); 
    $(".owl-nav button").append(owl_custom_arrow);
    $('.starRatings').append(starRatingsIcon+starRatingsIcon+starRatingsIcon+starRatingsIcon+starRatingsIcon);
	
    setTimeout(function(){
      console.log(starRatingsIcon);
        $('.starLabel').append(starRatingsIcon);
        if(starRatingsIcon != undefined) {
            $('.product_title_star_ratings').append(starRatingsIcon+starRatingsIcon+starRatingsIcon+starRatingsIcon+starRatingsIcon);
            $('.product_title_star_count').text("");
            $('.product_title_star_count').append(''+totReviews+' reviews'); 
            $('.product-single__title').append("<div class='starRating-Wrapper'><div class='starRatings'>"+starRatingsIcon+starRatingsIcon+starRatingsIcon+starRatingsIcon+starRatingsIcon+"</div><span class='starRating-count'>"+totReviews+" reviews</span></div>");
        }
        
    },1000);	

       $('.starRatings svg *').css("fill", settings.starBgCol);

    if(avgStarCount == 1){
      $('.starRating-Wrapper').find('svg:nth-child(1) *').css("fill", settings.selectedStarBgCol); 
    } else if(avgStarCount == 2){
      $('.starRating-Wrapper').find('svg:nth-child(1) *').css("fill", settings.selectedStarBgCol); 
      $('.starRating-Wrapper').find('svg:nth-child(2) *').css("fill", settings.selectedStarBgCol);         
    } else if(avgStarCount == 3){
      $('.starRating-Wrapper').find('svg:nth-child(1) *').css("fill", settings.selectedStarBgCol); 
      $('.starRating-Wrapper').find('svg:nth-child(2) *').css("fill", settings.selectedStarBgCol);         
      $('.starRating-Wrapper').find('svg:nth-child(3) *').css("fill", settings.selectedStarBgCol);         
    } else if(avgStarCount == 4){
      $('.starRating-Wrapper').find('svg:nth-child(1) *').css("fill", settings.selectedStarBgCol); 
      $('.starRating-Wrapper').find('svg:nth-child(2) *').css("fill", settings.selectedStarBgCol);         
      $('.starRating-Wrapper').find('svg:nth-child(3) *').css("fill", settings.selectedStarBgCol);         
      $('.starRating-Wrapper').find('svg:nth-child(4) *').css("fill", settings.selectedStarBgCol);         
    } else if(avgStarCount == 5){
      $('.starRating-Wrapper').find('svg:nth-child(1) *').css("fill", settings.selectedStarBgCol); 
      $('.starRating-Wrapper').find('svg:nth-child(2) *').css("fill", settings.selectedStarBgCol);         
      $('.starRating-Wrapper').find('svg:nth-child(3) *').css("fill", settings.selectedStarBgCol);         
      $('.starRating-Wrapper').find('svg:nth-child(4) *').css("fill", settings.selectedStarBgCol);         
      $('.starRating-Wrapper').find('svg:nth-child(5) *').css("fill", settings.selectedStarBgCol);         
    }

  }

  function setting(){
    var data = $("#customer_review" ).find("input").serialize();
    $.ajax({
      type: "POST",
      url: 'https://waltt-review-server.herokuapp.com/get_store_details',
      async : false,
      dataType: 'json',
      data: data,
      success:function(obj) {
        var settings = obj.data.settings;
        var style = JSON.parse(obj.data.css_content);
        $('#review_settings').val(settings);
        $('#customer_product_review').append(style);
      }  
    });


  }

